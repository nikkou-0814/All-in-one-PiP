{
    "manifest_version": 3,
    "name": "All in one PiP",
    "version": "2.6",
    "description": "Videoタグさえあればピクチャーインピクチャーできる",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}
  